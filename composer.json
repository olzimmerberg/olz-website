{
  "name": "olzimmerberg/olz-website",
  "type": "project",
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": false,
      "symfony/flex": true,
      "symfony/runtime": true,
      "composer/installers": true,
      "oomphinc/composer-installers-extender": true
    },
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Hatt",
      "email": "allestuetsmerweh@gmail.com"
    },
    {
      "name": "Urs Utzinger"
    }
  ],
  "scripts": {
    "check_migrations": "./tools/scripts/check_migrations.sh",
    "dev_server": "./run.sh",
    "doctrine": "./tools/scripts/doctrine.sh",
    "fix": "php-cs-fixer fix --config=./.php_cs_config.php -v",
    "fixdiff": "php-cs-fixer fix --config=./.php_cs_config.php -v --dry-run --diff",
    "integration_tests": "./tools/scripts/integration_tests.sh",
    "migrations": "./tools/scripts/migrations.sh",
    "remote_migrations": "./tools/scripts/remote_migrations.sh",
    "take_screenshots": "./tools/scripts/take_screenshots.sh",
    "test": "./tools/scripts/test.sh",
    "to_external_id": "php ./tools/scripts/to_external_id.php",
    "unit_tests": "./tools/scripts/unit_tests.sh",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "requirements-checker": "script",
      "vendor/bin/requirements-checker": "php-script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "paragonie/sodium_compat": "^1.17",
    "php-webdriver/webdriver": "^1.8",
    "phpunit/phpunit": "^9",
    "symfony/browser-kit": "6.1.*",
    "symfony/css-selector": "6.1.*",
    "symfony/phpunit-bridge": "^6.1"
  },
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "allestuetsmerweh/php-deploy": "^2.0",
    "allestuetsmerweh/php-typescript-api": "^2.0",
    "artgris/filemanager-bundle": "^2.4",
    "doctrine/annotations": "^1.13",
    "doctrine/cache": "^2.2",
    "doctrine/doctrine-bundle": "^2.6",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/migrations": "^3.0",
    "doctrine/orm": "^2.6.2",
    "ifsnop/mysqldump-php": "^2.9",
    "league/commonmark": "^2.1",
    "league/flysystem": "^3.2",
    "league/flysystem-ftp": "^3.0",
    "league/flysystem-sftp-v3": "^3.2",
    "monolog/monolog": "^3.2",
    "php-imap/php-imap": "^5.0",
    "phpmailer/phpmailer": "^6.2",
    "psr/log": "^3.0",
    "sabre/dav": "^4.3",
    "symfony/apache-pack": "^1.0",
    "symfony/console": "6.1.*",
    "symfony/dotenv": "6.1.*",
    "symfony/filesystem": "6.1.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "6.1.*",
    "symfony/http-client": "6.1.*",
    "symfony/http-foundation": "^6.1",
    "symfony/maker-bundle": "^1.43",
    "symfony/monolog-bundle": "^3.8",
    "symfony/proxy-manager-bridge": "6.1.*",
    "symfony/requirements-checker": "^2.0",
    "symfony/runtime": "6.1.*",
    "symfony/yaml": "6.1.*"
  },
  "autoload": {
    "psr-4": {
      "Olz\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Olz\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.1.*"
    }
  }
}
